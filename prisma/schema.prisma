// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Lead {
  id              Int      @id @default(autoincrement())
  firstName       String
  lastName        String
  email           String   @unique
  phone           String
  wordpressPostId String?
  createdAt       DateTime @default(now())
}
